{
  "name": "api-worker",
  "private": true,
  "packageManager": "bun@1.3.9",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:worker": "bun --filter api-worker dev",
    "dev:ui": "bun --filter api-worker-ui dev -- --port 4173",
    "test": "vitest",
    "format": "bunx --bun biome format --write .",
    "lint": "bunx --bun biome lint --write .",
    "check": "bunx --bun biome check --write .",
    "typecheck": "bunx --bun tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@types/node": "^20.11.30",
    "typescript": "^5.4.5",
    "vitest": "^1.5.3"
  },
  "dependencies": {
    "wrangler": "4"
  }
}


